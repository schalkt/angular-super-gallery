{
    "name": "angular-super-gallery",
    "version": "2.2.27",
    "description": "AngularJS Super Gallery",
    "homepage": "https://projects.schalk.hu/angular-super-gallery/demo/",
    "repository": {
        "type": "git",
        "url": "https://github.com/schalkt/angular-super-gallery.git"
    },
    "keywords": [
        "angular",
        "image",
        "gallery",
        "thumbnails",
        "component",
        "modal",
        "bootstrap"
    ],
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "init": "npm prune && npm update && npm audit fix && npm run development",
        "release": "npm --no-git-tag-version version patch && npm run production"
    },
    "author": "Tamas Schalk (https://github.com/schalkt)",
    "license": "MIT",
    "main": "index.js",
    "devDependencies": {
        "@types/angular": "1.8.9",
        "@types/angular-animate": "1.5.14",
        "@types/bootstrap": "4.*",
        "@types/jquery": "3.5.30",
        "compression-webpack-plugin": "^11.1.0",
        "cross-env": "^7.0.3",
        "i": "^0.3.7",
        "laravel-mix": "^6.0.49",
        "laravel-mix-angular-templatecache": "^1.0.1",
        "lodash": "^4.17.21",
        "postcss": "^8.4.41",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.77.8",
        "sass-loader": "^16.0.0",
        "ts-loader": "^9.5.1",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "angular": "1.8.3",
        "angular-animate": "1.8.3",
        "angular-touch": "1.8.3",
        "bootstrap": "4.*",
        "font-awesome": "4.7.0",
        "jquery": "3.7.1",
        "popper.js": "1.16.1",
        "screenfull": "6.0.2"
    }
}
